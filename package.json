{
  "name": "@snowcoders/sortier",
  "version": "2.0.0",
  "description": "An opionated code sorter",
  "bin": {
    "sortier": "./bin/index.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf .nyc_output coverage dist",
    "cover": "nyc npm run mocha",
    "precommit": "lint-staged",
    "prepare": "npm run clean && npm run build",
    "start": "npm run build && node bin/index.js",
    "test": "npm run mocha",
    "mocha": "mocha --compilers ts-node/register --require source-map-support/register --full-trace --bail src/**/*.test.ts"
  },
  "lint-staged": {
    "**/*.ts": [
      "node bin/index.js",
      "git add"
    ]
  },
  "keywords": [
    "sort",
    "sorting",
    "import",
    "module"
  ],
  "author": "Snowcoders",
  "license": "MIT",
  "repository": "https://github.com/snowcoders/sortier",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.45",
    "@types/sinon": "^4.1.2",
    "chai": "^4.1.2",
    "husky": "^0.14.3",
    "jsdom": "^11.6.2",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-jsdom-launcher": "^6.1.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-typescript": "^3.0.9",
    "karma-typescript-es6-transform": "^1.0.3",
    "karma-typescript-preprocessor": "^0.3.1",
    "lint-staged": "^6.1.1",
    "mocha": "^4.1.0",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0"
  },
  "dependencies": {
    "@types/estree": "0.0.38",
    "@types/globby": "^6.1.0",
    "@types/node": "^9.4.6",
    "babylon": "^7.0.0-beta.34",
    "cosmiconfig": "^4.0.0",
    "flow-parser": "^0.64.0",
    "fs": "0.0.1-security",
    "globby": "^8.0.1",
    "rimraf": "^2.6.2",
    "typescript": "^2.6.2",
    "typescript-eslint-parser": "^12.0.0"
  }
}